<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>richard.wincewicz@ed.ac.uk</em:id>
		<em:name>Hiberlink Plugin for Zotero</em:name>
		<em:version>0.2</em:version>
		<em:description>A proactive archiving plugin for Zotero</em:description>
		<em:creator>Richard Wincewicz<br/>EDINA</em:creator>
                <em:homepageURL>http://hiberlink.org</em:homepageURL>
		<em:type>2</em:type> <!-- type=extension -->
		
		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>1.*</em:minVersion>
				<em:maxVersion>4.*</em:maxVersion>
			</Description>
		</em:requires>
    
		<!-- Requires Firefox or Zotero Standalone -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>18.*</em:minVersion>
				<em:maxVersion>33.*</em:maxVersion>
			</Description>
		</em:targetApplication>
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>3.*</em:minVersion>
				<em:maxVersion>4.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
